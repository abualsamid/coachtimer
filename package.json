    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run lint && npm run typecheck && npm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "happy-dom": "^20.3.3",
    "typescript": "^5.7.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "wrangler": "^4.35.0"
  }
}
